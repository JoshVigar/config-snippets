# General Utility
alias rprofile='. ~/.profile'
alias aliases='nvim ~/.bash_aliases'
alias vim='nvim'

# Bundler
alias be='bundle exec'

# Docker
alias dc='docker-compose'
alias dcu='docker-compose up'
alias dcd='docker-compose down'
alias dcr='docker-compose down && docker-compose up'

# Navigation
alias cd-src='cd ~/josh/src/'
alias cd-josh='cd ~/josh'
alias cd-eng-library='cd ~/josh/src/eng-library'
